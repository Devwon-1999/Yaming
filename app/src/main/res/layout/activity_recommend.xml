<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RecommendActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">


        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:text="금일의 식단 추천"
            android:textColor="@color/identityColor"
            android:textSize="30dp" />
        <TextView
            android:id="@+id/oneDayTotalCal"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:text="1047.30cal"
            android:textColor="@color/identityColor"
            android:textSize="25dp"
            android:onClick="clickoneDayTotalCal"
            />
        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:text="일일 권장 영양소              부족 영양소"
            android:textColor="@color/identityColor"
            android:textSize="20dp" />




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:orientation="horizontal"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"

            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.395">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="150dp"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.319">

                <TextView
                    android:id="@+id/tan"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="탄수화물"
                    android:textColor="@color/identityColor"
                    android:textSize="13dp" />

                <TextView
                    android:id="@+id/dan"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="단백질"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/ji"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="지방"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="140dp"
                android:layout_height="150dp"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.319">

                <TextView
                    android:id="@+id/eatTan"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="300g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/eatDan"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="70g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/eatJi"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="50g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="150dp"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.319">

                <TextView
                    android:id="@+id/bujoktan"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="탄수화물"
                    android:textColor="@color/identityColor"
                    android:textSize="13dp" />

                <TextView
                    android:id="@+id/bujokdan"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="단백질"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/bujokji"
                    android:layout_width="55dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="지방"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="140dp"
                android:layout_height="150dp"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.319">

                <TextView
                    android:id="@+id/bujokeatTan"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="138.18g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/bujokeatDan"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="14.16g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/bujokeatJi"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="30.14g"
                    android:textColor="@color/identityColor"
                    android:textSize="15dp" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_gravity="left"
                android:text="추천 식단"
                android:textColor="@color/identityColor"
                android:textSize="25dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"></LinearLayout>


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>